<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  
<!-- Local Manifest for DEVICES Template 
      use this only on FluentOS Repo Tree -->
  
<!-- Device Tree -->
 <project path="device/[brand]/[codename]" name="fluent-devices/android_device_[brand]_[codename]" remote="github" revision="fluent-1.0"/>
<!-- if this device have Common Device Tree -->
 <project path="device/[brand]/[common_device-tree]" name="fluent-devices/android_device_[brand]_[common_device-tree]" remote="github" revision="fluent-1.0"/>
 
<!-- Kernel tree -->
 <project path="kernel/[brand]/[codename or soc kernel tree]" name="fluent-devices/android_kernel_[brand]_[codename or soc kernel tree]" remote="github" revision="fluent-1.0"/>
 
<!-- Complements -->
 <!-- Place your 3th party complements here -->
</manifest>
